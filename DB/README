CREATE DATABASE movieDB;


CREATE TABLE film (
  id INT PRIMARY KEY AUTO_INCREMENT,
  name VARCHAR(2550) NOT NULL,
  year INT,
  image VARCHAR(2550),
  link VARCHAR(2550) NOT NULL,
  ytlink VARCHAR(2550) NOT NULL
);

CREATE TABLE Tvseries (
  id INT PRIMARY KEY AUTO_INCREMENT,
  name VARCHAR(2550) NOT NULL,
  year INT,
  image VARCHAR(2550),
  link VARCHAR(2550) NOT NULL,
  ytlink VARCHAR(2550) NOT NULL
);


